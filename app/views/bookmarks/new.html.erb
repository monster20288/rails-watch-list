<div class="container">
  <div class="row ">
    <div class="col-md-12">
     <h1 class="mb-4">New List </h1>
        <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.label :comment, class: 'form-label' %><br>
        <%= f.text_field :comment, class: 'form-control' %><br>
        <%= f.association :movie %>
        <%= f.button :submit, class:'btn btn-primary' %>
       <% end %>
    </div>
  </div>
</div>
